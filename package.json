{
  "name": "urlshortener",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run i18n:validate && next build",
    "start": "next start",
    "lint": "next lint",
    "dev:debug": "cross-env NEXT_DEBUG_TURBOPACK=1 next dev",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "test": "npx jest",
    "worker": "npx tsx src/scripts/runAnalyticsWorker.ts",
    "worker:rabbit": "npx tsx --env-file=.env.local src/scripts/runRabbitWorker.ts",
    "setup:rabbit": "npx tsx src/scripts/setupRabbitMQ.ts",
    "test:emit": "npx tsx src/scripts/manualTestEmit.ts",
    "seed:links": "npx tsx --env-file=.env.local src/scripts/stress/seedLinks.ts",
    "stress:rabbit": "npx tsx --env-file=.env.local src/scripts/stress/stressRabbitWorker.ts",
    "worker:go": "cd go-worker && go run .",
    "i18n:extract": "tsx src/scripts/i18n/extract.ts",
    "i18n:validate": "tsx src/scripts/i18n/validate.ts",
    "migrate:avatars": "tsx src/scripts/migrateAvatarsToR2.ts",
    "r2:cleanup-avatars": "tsx src/scripts/r2/cleanupOrphanAvatars.ts"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.11.1",
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@paddle/paddle-js": "^1.4.1",
    "@uploadthing/react": "^7.3.1",
    "@upstash/redis": "^1.36.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "amqplib": "^0.10.9",
    "bcryptjs": "^3.0.3",
    "html2canvas": "^1.4.1",
    "k6": "^0.0.0",
    "lodash.debounce": "^4.0.8",
    "lru-cache": "^11.2.4",
    "lucide-react": "^0.511.0",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.2",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "next-intl": "^4.6.1",
    "nodemailer": "^7.0.12",
    "otplib": "^12.0.1",
    "p-limit": "^7.2.0",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-globe.gl": "^2.33.2",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "swr": "^2.3.8",
    "three": "^0.176.0",
    "uploadthing": "^7.7.2",
    "uuid": "^13.0.0",
    "validator": "^13.15.26",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/amqplib": "^0.10.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/html2canvas": "^0.5.35",
    "@types/jest": "^30.0.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.4",
    "@types/qrcode": "^1.5.6",
    "@types/qrcode.react": "^1.0.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.10",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
